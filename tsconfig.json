{
  "compilerOptions": {
    "target": "es2019",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "sourceMap": true,
    "outDir": "dist",
    "rootDir": ".",
    "strict": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "typeRoots": [
      "./node_modules/@types"
    ]
  },
  "include": [
    "nodes/**/*.ts",
    "credentials/**/*.ts",
    "types.d.ts"
  ],
  "exclude": [
    "node_modules"
  ]
} 